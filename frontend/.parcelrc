{
  "extends": "@parcel/config-default",
  "compressors": {
    "*.{html,css,js,svg,map}": [
      "...",
      "@parcel/compressor-gzip",
      "@parcel/compressor-brotli"
    ]
  },
  "transformers": {
    "*.stpl": [
      "@parcel/transformer-posthtml",
      "@parcel/transformer-html"
    ]
  },
  "packagers": {
    "*.stpl": "parcel-packager-sailfish",
  }
}
