{
  "name": "posthtml-lucide",
  "description": "PostHTML plugin for pulling in Lucide icons",
  "version": "0.0.0",
  "license": "MIT",
  "author": "PostHTML (https://github.com/posthtml)",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "./dist/index.mjs",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
