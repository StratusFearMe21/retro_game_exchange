    "lint": "biome lint ./lib ./test"
  },
  "keywords": [
    "html",
    "posthtml",
    "posthtml-plugin"
  ],
  "devDependencies": {
    "@biomejs/biome": "2.2.6",
    "posthtml": "^0.16.7",
    "tsdown": "0.20.0-beta.3",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "change-case": "^5.4.4",
    "lucide": "^0.556.0",
    "lucide-static": "^0.556.0",
    "posthtml-parser": "^0.12.1"
  }
